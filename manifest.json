{
  "manifest_version": 2,
  "name": "Forecast",
  "description": "Forecast demo using angular",
  "version": "0.1",
  "permissions": ["http://api.wunderground.com/api/"],
  "background": {
    "scripts": ["js/vendor/angular.min.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  }
}